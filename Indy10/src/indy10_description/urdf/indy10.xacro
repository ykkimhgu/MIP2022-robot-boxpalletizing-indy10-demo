  </link>
  
  <link name = "tcp" />

  <joint name = "global" type = "fixed">
  	<parent link = "world"/>
  	<child link = "link0"/>
    <origin
      xyz="0 0 0"
      rpy="0 0 0" />
  </joint>
	
  <joint name = "joint0" type = "revolute">
  	<parent link = "link0"/>
  	<child link = "link1"/>
    <origin
      xyz="0 0 0"
      rpy="${PI/2} 0 0" />
    <axis
      xyz="0 1 0" />
    <limit
      lower="-3.14"
      upper="3.14"
      effort="420"
      velocity="1.57" />
  </joint>

  <joint name = "joint1" type = "revolute">
  	<parent link = "link1"/>
  	<child link = "link2"/>
    <origin
      xyz="-0.084469 0.26 0"
      rpy="0 0 0" />
    <axis
      xyz="-1 0 0" />
    <limit
      lower="-2.35"
      upper="2.35"
      effort="420"
      velocity="1.57" />
  </joint>

  <joint name = "joint2" type = "revolute">
  	<parent link = "link2"/>
  	<child link = "link3"/>
    <origin
      xyz="0 0.48 0"
      rpy="${-PI} 0 0" />
    <axis
      xyz="-1 0 0" />
    <limit
      lower="-2.61"
      upper="2.61"
      effort="200"
      velocity="1.57" />
  </joint>

  <joint name = "joint3" type = "revolute">
  	<parent link = "link3"/>
  	<child link = "link4"/>
    <origin
      xyz="0.084469 -0.12571 0"
      rpy="${-PI} 0 0" />
    <axis
      xyz="0 1 0" />
    <limit
      lower="-3.14"
      upper="3.14"
      effort="200"
      velocity="1.57" />
  </joint>

  <joint name = "joint4" type = "revolute">
  	<parent link = "link4"/>
  	<child link = "link5"/>
    <origin
      xyz="0 0.39429 0"
      rpy="${PI} 0 0" />
    <axis
      xyz="-1 0 0" />
    <limit
      lower="-2.56"
      upper="2.56"
      effort="69"
      velocity="1.57" />
  </joint>

  <joint name = "joint5" type = "revolute">
  	<parent link = "link5"/>
  	<child link = "link6"/>
    <origin
      xyz="0 -0.111 0"
      rpy="${PI} ${PI-PI/6} 0" />
    <axis
      xyz="0 1 0" />
    <limit
      lower="-3.14"
      upper="3.14"
      effort="69"
      velocity="1.57" />
  </joint>

  <joint name = "tcp" type = "fixed">
  	<parent link = "link6"/>
  	<child link = "tcp"/>
  	<origin xyz = "0 0.089 0" rpy = "${-PI/2} ${5*PI/6} 0"/>
      <!--<origin xyz = "0 0.089 0" rpy = "${-PI/2} ${-2*PI/3} 0"/>-->
  </joint>

</robot>